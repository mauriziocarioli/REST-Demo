<kmodule xmlns="http://www.drools.org/xsd/kmodule" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <kbase name="default-kbase" default="true" eventProcessingMode="cloud" equalsBehavior="equality">
      <ksession name="default-stateless-ksession" type="stateless" default="true" clockType="realtime">
        <workItemHandlers>
          <workItemHandler type="org.jbpm.process.workitem.rest.RESTWorkItemHandler" name="REST"/>
        </workItemHandlers>
      </ksession>
      <ksession name="default-stateful-ksession" type="stateful" default="false" clockType="pseudo"/>
    </kbase>
  </kmodule>